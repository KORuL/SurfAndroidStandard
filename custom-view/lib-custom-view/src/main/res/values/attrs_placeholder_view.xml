        <attr name="pvNoInternetSubtitle" format="string" />
        <attr name="pvNoInternetButtonText" format="string" />
        <attr name="pvNoInternetSecondButtonText" format="string" />
        <attr name="pvNoInternetImage" format="reference" />

        <attr name="pvErrorTitle" format="string" />
        <attr name="pvErrorSubtitle" format="string" />
        <attr name="pvErrorButtonText" format="string" />
        <attr name="pvErrorSecondButtonText" format="string" />
        <attr name="pvErrorImage" format="reference" />

