        <attr name="pvOpaqueBackground" format="reference" />
        <attr name="pvTransparentBackgroundColor" format="color" />
        <attr name="pvTransparentBackground" format="reference" />
        <attr name="pvProgressBarColor" format="color" />
        <attr name="pvProgressBarWidth" format="dimension" />
        <attr name="pvProgressBarHeight" format="dimension" />
        <attr name="pvProgressBarType" format="enum">
            <enum name="StandardCircleIndicator" value="0" />
            <enum name="BallPulseIndicator" value="1" />
            <enum name="BallGridPulseIndicator" value="2" />
            <enum name="BallClipRotateIndicator" value="3" />
            <enum name="BallClipRotatePulseIndicator" value="4" />
            <enum name="SquareSpinIndicator" value="5" />
            <enum name="BallClipRotateMultipleIndicator" value="6" />
            <enum name="BallPulseRiseIndicator" value="7" />
            <enum name="BallRotateIndicator" value="8" />
            <enum name="CubeTransitionIndicator" value="9" />
            <enum name="BallZigZagIndicator" value="10" />
            <enum name="BallZigZagDeflectIndicator" value="11" />
            <enum name="BallTrianglePathIndicator" value="12" />
            <enum name="BallScaleIndicator" value="13" />
            <enum name="LineScaleIndicator" value="14" />
