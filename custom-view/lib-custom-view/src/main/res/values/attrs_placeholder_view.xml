            <enum name="BallScaleRippleMultipleIndicator" value="22" />
            <enum name="BallSpinFadeLoaderIndicator" value="23" />
            <enum name="LineSpinFadeLoaderIndicator" value="24" />
            <enum name="TriangleSkewSpinIndicator" value="25" />
            <enum name="PacmanIndicator" value="27" />
            <enum name="BallGridBeatIndicator" value="28" />
            <enum name="SemiCircleSpinIndicator" value="29" />
