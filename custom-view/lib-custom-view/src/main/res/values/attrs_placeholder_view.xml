        <attr name="pvTitle" format="string" />
        <attr name="pvSubtitle" format="string" />
        <attr name="pvButtonText" format="string" />
        <attr name="pvSecondButtonText" format="string" />
        <attr name="pvImage" format="reference" />

        <attr name="pvEmptyTitle" format="string" />
        <attr name="pvEmptySubtitle" format="string" />
        <attr name="pvEmptyButtonText" format="string" />
        <attr name="pvEmptySecondButtonText" format="string" />
        <attr name="pvEmptyImage" format="reference" />

        <attr name="pvNotFoundTitle" format="string" />
