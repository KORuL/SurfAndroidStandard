<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PostfixEditText">   <!-- todo удалить если неунжно PostfixEditText -->
        <attr name="postfixText" format="string" />
        <attr name="postfixTextColor" format="color" />
        <attr name="postfixPadding" format="dimension" />
        <attr name="postfixTextAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="KeyView">
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="font" format="reference" />
    </declare-styleable>

    <declare-styleable name="KeyBoardView">
        <attr name="verticalSpace" format="dimension" />
        <attr name="horizontalSpace" format="dimension" />
        <attr name="btn_width" format="dimension" />
        <attr name="btn_height" format="dimension" />
    </declare-styleable>
</resources>